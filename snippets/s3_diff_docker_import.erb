s3_diff_docker_import_base_fn() {
<%= get_from_s3(data[:base_image]) %>
}
s3_diff_docker_import_diff_fn() {
<%= get_from_s3(data[:diff_image]) %>
}
s3_diff_docker_import_base_fn | gunzip -c > /opt/dockly/base_image.tar
size=$(stat -f "%z" /opt/dockly/base_image.tar)
head_size=$(($size - 1024))
(head -c $head_size "/opt/dockly/base_image.tar"; s3_diff_docket_import_diff_fn | gunzip -c) | <%= docker_import(data[:repo], data[:tag]) %>
